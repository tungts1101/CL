    "n_centroids": 10,

    "pretrained": true,
    "drop": 0.0,
    "drop_path": 0.0,
    "ffn_num": 16,

    "crct_epochs": 10,
    "ca_sample_per_cls": 512,
    "ca_batch_size": 128,
    "ca_lr": 0.0005,
