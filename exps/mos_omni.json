{
    "prefix": "reproduce",
    "dataset": "omnibenchmark",
    "memory_size": 0,
    "memory_per_class": 0,
    "fixed_memory": false,
    "shuffle": true,
    "init_cls": 30,
    "increment": 30,
    "model_name": "mos",
    "backbone_type": "vit_base_patch16_224_mos",
    "device": ["0"],
    "seed": [1993, 1994, 1995],
    
    "tuned_epoch": 10,
    "init_lr": 0.01,
    "batch_size": 16,
    "weight_decay": 0.0005,
    "min_lr": 0,
    "optimizer": "sgd",
    "scheduler": "cosine",
    "reinit_optimizer": true,
    "init_milestones": [10],
    "init_lr_decay": 0.1,
    "reg": 0.1,
    "adapter_momentum": 0.1,
